@page
@model AspNetCoreIdentity.Pages.LogOutModel
@{
}


<div>
    <div class="container">
        <div class="md-12">
            <h1>Login</h1>
            <form method="post">
                <div class="md-3">
                   <h1>Are you Sure you want to Logout?</h1>
                </div>
                <div class="md-3">
                    <p>
                        <button class="btn btn-primary" asp-page-handler="LogOut">Yes</button>
                        <button class="btn btn-light ml-5 border-dark" asp-page-handler="DontLogOut">No </button>
                    </p>   
                </div>
            </form>
        </div>
    </div>
</div>